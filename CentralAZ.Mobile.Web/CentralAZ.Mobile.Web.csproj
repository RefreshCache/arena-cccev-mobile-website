  <Target Name="AfterBuild" DependsOnTargets="BuildPackageZip">
  </Target>
  <Target Name="MvcBuildViews" AfterTargets="AfterBuild" Condition="'$(MvcBuildViews)'=='true'">
    <AspNetCompiler VirtualPath="temp" PhysicalPath="$(WebProjectOutputDir)" />
  </Target>
 <!-- Define a builds folder where the zipped releases will be put -->
  <PropertyGroup>
    <BuildDir>..\builds</BuildDir>
  </PropertyGroup>
  <!-- Clean old stuff -->
  <Target Name="DeleteFiles">
    <ItemGroup>
      <OldFiles Include="$(BuildDir)\mobile\**" />
    </ItemGroup>
    <Delete Files="@(OldFiles)" />
  </Target>
  <!-- Create a deployable "mobile" folder with the necessary files -->
  <Target Name="CopyToMobile" DependsOnTargets="DeleteFiles">
    <MakeDir Directories="$(BuildDir)\mobile" Condition="!Exists('$(BuildDir)\mobile')" />
    <MakeDir Directories="$(BuildDir)\mobile\bin" Condition="!Exists('$(BuildDir)\mobile\bin')" />
    <MakeDir Directories="$(BuildDir)\mobile\Content" Condition="!Exists('$(BuildDir)\mobile\Content')" />
    <MakeDir Directories="$(BuildDir)\mobile\Scripts" Condition="!Exists('$(BuildDir)\mobile\Scripts')" />
    <MakeDir Directories="$(BuildDir)\mobile\Views" Condition="!Exists('$(BuildDir)\mobile\Views')" />
    <ItemGroup>
      <BinFiles Include="$(OutputPath)\*.dll" />
      <ContentFiles Include=".\Content\**\*.*" Exclude=".\Content\**\_sgbak\**" />
      <ScriptsFiles Include=".\Scripts\**\*.*" Exclude=".\Scripts\**\_sgbak\**"/>
      <ViewsFiles Include=".\Views\**\*.*" Exclude=".\Views\**\_sgbak\**"/>
      <RootFiles Include=".\*.asax;Web.Release.config" />
    </ItemGroup>
    <Message Text="Copying DLLs..." />
    <Copy SourceFiles="@(BinFiles)" DestinationFolder="$(BuildDir)\mobile\bin" />
    <Message Text="Copying Content folder..." />
    <Copy SourceFiles="@(ContentFiles)" DestinationFolder="$(BuildDir)\mobile\Content" />
    <Message Text="Copying Scripts folder..." />
    <Copy SourceFiles="@(ScriptsFiles)" DestinationFolder="$(BuildDir)\mobile\Scripts" />
    <Message Text="Copying Views..." />
    <Copy SourceFiles="@(ViewsFiles)" DestinationFolder="$(BuildDir)\mobile\Views" />
    <Message Text="Copying Root Files..." />
    <Copy SourceFiles="@(RootFiles)" DestinationFolder="$(BuildDir)\mobile" />
  </Target>
  <!-- Create the package zip file -->
  <Target Name="BuildPackageZip" DependsOnTargets="CopyToMobile" Condition="'$(Configuration)' == 'Release'">
    <ItemGroup>
      <ZipFiles Include="$(BuildDir)\**\*.*" Exclude="*.zip" />
    </ItemGroup>
    <Version BuildType="Automatic" RevisionType="Automatic" Major="2012" Minor="1">
      <Output TaskParameter="Major" PropertyName="Major" />
      <Output TaskParameter="Minor" PropertyName="Minor" />
      <Output TaskParameter="Build" PropertyName="Build" />
      <Output TaskParameter="Revision" PropertyName="Revision" />
    </Version>
    <Message Text="Version: $(Major).$(Minor).$(Build).$(Revision)" />
    <Zip Files="@(ZipFiles)" WorkingDirectory="$(BuildDir)\" ZipFileName="$(BuildDir)\CentralAZ.Mobile.Web-$(Major).$(Minor).$(Build).$(Revision).zip" ZipLevel="9" />
  </Target>
